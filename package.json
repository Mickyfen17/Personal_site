{
  "name": "personal_site",
  "version": "1.0.0",
  "description": "My Personal Website",
  "main": "index.tsx",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run lint & npm run build",
    "pretty": "prettier --print-width 100 --single-quote --trailing-comma none --jsx-single-quote true --write \"src/**/*.{ts,tsx}\"",
    "start": "webpack serve --mode development --env=env=dev",
    "build": "webpack --mode production --env=env=prod",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "mocha --require test/helpers/setup.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "analyize": "webpack-bundle-analyzer ./dist/stats.json -m static"
  },
  "lint-staged": {
    "src/**/*.tsx?$": [
      "npm run pretty",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mickyfen17/Personal_site"
  },
  "author": "Mike Fenwick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mickyfen17/Personal_site/issues"
  },
  "homepage": "https://mickyfen17.github.io/Personal_site/",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-refresh": "^0.9.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.3",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
