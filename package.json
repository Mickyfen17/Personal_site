{
  "name": "personal_site",
  "version": "1.0.0",
  "description": "My Personal Website",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "pretty": "prettier --print-width 80 --single-quote --trailing-comma es5 --write \"lib/**/*.js\"",
    "start": "webpack-dev-server --mode development --env.env=dev",
    "build": "webpack -p --mode production --env.env=prod",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "mocha --require test/helpers/setup.js",
    "analyize": "webpack-bundle-analyzer ./dist/stats.json -m static"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mickyfen17/Personal_site"
  },
  "author": "Mike Fenwick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mickyfen17/Personal_site/issues"
  },
  "homepage": "https://mickyfen17.github.io/Personal_site/",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^5.0.1",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
